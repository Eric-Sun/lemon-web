<script type="text/javascript">
    var apk2exeNode = $("#apk2exe")[0];
    var exeWaitNode = $("#exeWait")[0];
    var exeDownloadNode = $("#exeDownload")[0];
    var exeFailNode = $("#exeFail")[0];
    var exeDownloadUrl = "";
    var isCheckPort = false;
    var apkNode = null;
    var tjurl = "";

    $(document).ready( function() {

        $("#apk2exe-close").bind("click", function(){
            apk2exeCloseDialog();
        });

        $("#apk2exeDownloadBtn").bind("click", function(){
            if(exeDownloadUrl){
                apk2exeCloseDialog();
                window.open(exeDownloadUrl);
                if(tjurl){
                    $.get(tjurl + "&r=" + Math.random());
                }
            }
        });

        $('.event-exe-install').bind('click', function(e){
            if(isCheckPort){return;}
            var element = $(this)[0];
            apkNode = element;
            isCheckPort = true;
            apkNode.innerHTML = "加载中...";
            exeWaitNode.style.display = "block";
            exeDownloadNode.style.display = "none";
            exeFailNode.style.display = "none";
            tjurl = element.getAttribute("tjurl");

            var packageInfo = {
                "restype": "apk",
                "sname": element.getAttribute("data-sname"),
                "package": element.getAttribute("data-package"),
                "versionname": element.getAttribute("data-versionname"),
                "versioncode": element.getAttribute("data-versioncode"),
                "download_url": element.getAttribute("data-download_url"),
                "icon": element.getAttribute("data-icon"),
                "type": "apk",
                "size": element.getAttribute("data-size"),
                "updatetime": element.getAttribute("data-updatetime"),
                "from": "1000364n"
            }

            var isInstakkSDK = bdApk2Exe.download(packageInfo,
                    function(flag){
                        if(flag){
                            if(tjurl){
                                $.get(tjurl + "&r=" + Math.random());
                            }
                            apk2exeCloseDialog();
                        } else {
                            var appNameTitle = packageInfo.sname;
                            var appNameIcon = appNameTitle.length > 4 ? appNameTitle.substring(0,4)+"..." : appNameTitle;
                            if(appNameTitle.length > 6){
                                appNameTitle = appNameTitle.substring(0,6)+"...";
                            }
                            document.getElementById("apk2exe-title-sname").innerHTML = appNameTitle;
                            document.getElementById("apk2exe-content-sname").innerHTML = appNameIcon;
                            if(packageInfo.icon) {
                                document.getElementById("apk2exe_app_icon").src =packageInfo.icon;
                            }
                            apk2exeNode.style.display = "block";
                            var isIE6 = !!window.ActiveXObject&&!window.XMLHttpRequest;
                            if(isIE6){
                                var dialogNode = document.getElementById("apk2exe-dialog");
                                var maskNode = document.getElementById("apk2exe-mask");
                                maskNode.style.position = "absolute";
                                dialogNode.style.position = "absolute";
                                maskNode.style.height=document.body.offsetHeight+"px";
                                dialogNode.style.top=(document.documentElement.scrollTop + (document.documentElement.clientHeight-dialogNode.offsetHeight)/2)+"px";
                                dialogNode.style.left=(document.documentElement.scrollLeft + (document.documentElement.clientWidth-dialogNode.offsetWidth)/2)+"px";
                            }
                        }
                    }, function(url){
                        if(url == "fail"){
                            exeWaitNode.style.display = "none";
                            exeFailNode.style.display = "block";
                            exeDownloadNode.style.display = "none";
                        } else {
                            exeDownloadUrl = url;
                            exeWaitNode.style.display = "none";
                            exeFailNode.style.display = "none";
                            exeDownloadNode.style.display = "block";
                        }
                    }
            );
            if(isInstakkSDK){
                apk2exeCloseDialog();
            }
        });
    });

    function apk2exeCloseDialog(){
        if(apkNode.className.indexOf("info-download") != -1){
            apkNode.innerHTML = "直接安装";
        } else if(apkNode.className.indexOf("btn-download-app") != -1 || apkNode.className.indexOf("s-index-icon") != -1){
            apkNode.innerHTML = "安&nbsp;装";
        }
        isCheckPort = false;
        apk2exeNode.style.display = "none";
        exeWaitNode.style.display = "block";
        exeDownloadNode.style.display = "none";
        exeFailNode.style.display = "none";
    }
</script>

